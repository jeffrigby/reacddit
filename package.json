{
  "name": "reacddit",
  "version": "1.0.0",
  "description": "A Reddit client built in reactjs.",
  "scripts": {
    "start": "concurrently \"npm run start-client\"  \"npm run start-api\"",
    "start-client": "cd client && npm start",
    "build-client": "cd client && npm run build",
    "start-api": "cd api && npm start",
    "outdated-all": "find . -maxdepth 2 -name package.json -execdir npm outdated \\;",
    "update-all": "find . -maxdepth 2 -name package.json -execdir npm update \\;"
  },
  "keywords": [
    "es6",
    "react",
    "redux",
    "reddit"
  ],
  "author": "Jeff Rigby",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.0.1",
    "esbuild": "^0.23.0",
    "eslint": "^8.1.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^4.0.2",
    "prettier": "^3.3.3"
  },
  "workspaces": [
    "client",
    "api"
  ],
  "dependencies": {
    "postcss-preset-env": "^10.0.2",
    "sass-loader": "^16.0.1"
  }
}
