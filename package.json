{
  "name": "reacddit",
  "version": "1.0.0",
  "description": "A Reddit client built in reactjs.",
  "type": "module",
  "scripts": {
    "setup": "tsx scripts/setup-wizard.ts",
    "start": "tsx scripts/start-dev.ts",
    "start-proxy": "cd proxy && npm start",
    "start-client": "cd client && npm start",
    "build-client": "cd client && npm run build",
    "start-api": "cd api && npm start",
    "outdated-all": "find . -maxdepth 2 -name package.json -execdir npm outdated \\;",
    "update-all": "find . -maxdepth 2 -name package.json -execdir npm update \\;"
  },
  "keywords": [
    "es6",
    "react",
    "redux",
    "reddit"
  ],
  "author": "Jeff Rigby",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "@inquirer/prompts": "^7.10.1",
    "@types/node": "^22.10.5",
    "concurrently": "^9.0.1",
    "open": "^10.1.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
