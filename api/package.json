{
  "name": "reacddit_api",
  "version": "1.0.0",
  "description": "reacddit Oauth2 API",
  "type": "module",
  "license": "MIT",
  "author": "",
  "main": "index.ts",
  "scripts": {
    "start": "tsx watch index.ts",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "prettier --write 'src/**/*.{ts,js,mjs}' && eslint --fix 'src/**/*.{ts,js,mjs}'",
    "type-check": "tsc --noEmit",
    "pm2-start": "pm2 start index.ts --name reacddit_api --interpreter tsx",
    "pm2-stop": "pm2 stop reacddit_api",
    "pm2-log": "pm2 log reacddit_api",
    "pm2-delete": "pm2 delete reacddit_api",
    "pm2-restart": "pm2 restart 'reacddit_api'"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^2.0.2",
    "@aws-lambda-powertools/parameters": "^2.0.2",
    "@aws-sdk/client-ssm": "^3.514.0",
    "@koa/router": "^14.0.0",
    "axios": "^1.2.1",
    "chalk": "^5.0.1",
    "dotenv": "^17.0.1",
    "dotenv-defaults": "^5.0.0",
    "esbuild": "^0.25.0",
    "koa": "^3.0.0",
    "koa-logger": "^4.0.0",
    "koa-session": "^7.0.2",
    "pm2": "^6.0.5",
    "qs": "^6.11.1",
    "serverless-http": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@types/aws-lambda": "^8.10.145",
    "@types/koa": "^3.0.1",
    "@types/koa__router": "^12.0.4",
    "@types/koa-logger": "^3.1.5",
    "@types/node": "^22.10.5",
    "@types/qs": "^6.9.17",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint": "^9.10.0",
    "globals": "^16.0.0",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^4.0.7"
  }
}
