{
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "mocha": true
    },
    "globals": {
      "jQuery": true
    },
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
      "global-require": 0,
      "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
      "no-unused-vars": ["error", { "args": "none" }],
      "react/jsx-filename-extension": 0,
      "react/forbid-prop-types": 0,
      "jsx-a11y/media-has-caption": 0,
      "max-len": ["error", 200]
    }
}
